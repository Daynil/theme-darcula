{
  "name": "@telamonian/theme-darcula",
  "version": "0.20.0",
  "description": "Darcula theme for Jupyterlab. Modelled after the classic Intellij theme.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme",
    "darcula",
    "darcula-theme"
  ],
  "homepage": "https://github.com/telamonian/theme-darcula",
  "bugs": {
    "url": "https://github.com/telamonian/theme-darcula/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Max Klein",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telamonian/theme-darcula.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~3.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themeDir": "style",
    "themePath": "style/index.css"
  }
}
